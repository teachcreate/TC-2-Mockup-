                }, 1000 + (index * 200));
            });

            // Add pulse animation to launch button
            const launchButton = document.querySelector('.launch-button');
            setInterval(() => {
                launchButton.style.transform = 'scale(1.02)';
                setTimeout(() => {
                    launchButton.style.transform = 'scale(1)';
                }, 200);
            }, 3000);
        });
